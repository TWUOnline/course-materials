@article{graenRelationshipbasedApproachLeadership1995,
  title = {Relationship-Based Approach to Leadership: {{Development}} of Leader-Member Exchange ({{LMX}}) Theory of Leadership over 25 Years: {{Applying}} a Multi-Level Multi-Domain Perspective},
  shorttitle = {Relationship-Based Approach to Leadership},
  author = {Graen, George B. and {Uhl-Bien}, Mary},
  year = {1995},
