  keywords = {big data,Coronary Artery Disease,Cross-Sectional Studies,Data Collection,emotions,Emotions,Female,heart disease,Humans,language,Language,Male,Models Psychological,Models Statistical,open data,open materials,Regression Analysis,risk factors,Risk Factors,social media,Social Media,United States,well-being},
