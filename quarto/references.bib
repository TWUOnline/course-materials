@article{lumpkinExplicatingSynergiesSelfdetermination2018,
  title = {Explicating the Synergies of Self‐determination Theory, Ethical Leadership, Servant Leadership, and Emotional Intelligence},
  author = {Lumpkin, Angela and Achen, Rebecca M.},
  date = {2018},
  journaltitle = {Journal of Leadership Studies},
  shortjournal = {Journal of Leadership Studies},
  volume = {12},
  number = {1},
  pages = {6--20},
  publisher = {John Wiley \& Sons},
