@article{limEffectivenessPositivePsychology2023,
  title = {The Effectiveness of Positive Psychology Interventions for Promoting Well-Being of Adults Experiencing Depression Compared to Other Active Psychological Treatments: {{A}} Systematic Review and Meta-Analysis},
  shorttitle = {The Effectiveness of Positive Psychology Interventions for Promoting Well-Being of Adults Experiencing Depression Compared to Other Active Psychological Treatments},
  author = {Lim, Wei Loong and Tierney, Stephanie},
  date = {2023-01-01},
  journaltitle = {Journal of Happiness Studies},
