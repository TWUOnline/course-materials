@article{segerstromOptimismImmunityPositive2005,
  title = {Optimism and Immunity: Do Positive Thoughts Always Lead to Positive Effects?},
  shorttitle = {Optimism and Immunity},
  author = {Segerstrom, Suzanne C.},
  date = {2005-05},
  journaltitle = {Brain, Behavior, and Immunity},
  shortjournal = {Brain Behav Immun},
  volume = {19},
  number = {3},
  eprint = {15797306},
  eprinttype = {pmid},
  pages = {195--200},
  issn = {0889-1591},
  doi = {10.1016/j.bbi.2004.08.003},
  abstract = {The effects of dispositional optimism, as defined by generalized positive expectations for the future, on physical health are mixed, especially in diseases that can be immunologically mediated such as HIV and cancer. Both experimental and naturalistic studies show that optimism is negatively related to measures of cellular immunity when stressors are difficult (e.g., complex, persistent, and uncontrollable) but positively related when stressors are easy (e.g., straightforward, brief, and controllable). Although the negative relationship between optimism and immunity has been attributed to the violation of optimists' positive expectancies and subsequent disappointment, empirical evidence suggests that it is more likely to be a consequence of optimists' greater engagement during difficult stressors. For example, negative mood does not account for the effect, but conscientiousness, a personality facet related to engagement, does. The mixed immunological correlates of optimism may explain why it does not consistently predict better disease outcomes.},
  langid = {english},
  pmcid = {PMC1948078},
  keywords = {Adaptation Psychological,Attitude to Health,Emotions,Health Status,HIV Infections,Humans,Neoplasms,Personality,Psychoneuroimmunology,Stress Physiological},
  file = {/Users/chloe.chang/Zotero/storage/6NZLT6LE/Segerstrom - 2005 - Optimism and immunity do positive thoughts always lead to positive effects.pdf}
}

