  title = {Positive Psychology in Clinical Practice},
  author = {Duckworth, Angela Lee and Steen, Tracy A. and Seligman, Martin E. P.},
  date = {2005-04-27},
  journaltitle = {Annual Review of Clinical Psychology},
  volume = {1},
