  title = {The Relationship between Innovation and Sustainability: {{A}} Meta-Analytic Study},
  shorttitle = {The Relationship between Innovation and Sustainability},
  author = {Kuzma, Edson and Padilha, Luccas Santin and Sehnem, Simone and Julkovski, Dulcimar Jos{\'e} and Roman, Darlan Jos{\'e}},
