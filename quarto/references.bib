@article{gismero-gonzalezInterpersonalOffensesPsychological2020,
  title = {Interpersonal {{Offenses}} and {{Psychological Well-Being}}: {{The Mediating Role}} of {{Forgiveness}}},
  shorttitle = {Interpersonal {{Offenses}} and {{Psychological Well-Being}}},
  author = {{Gismero-Gonz{\'a}lez}, Elena and J{\'o}dar, Rafael and Mart{\'i}nez, Mar{\'i}a Pilar and Carrasco, Mar{\'i}a Jos{\'e} and Cagigal, Virginia and {Prieto-Urs{\'u}a}, Mar{\'i}a},
  year = {2020},
  month = jan,
  journal = {Journal of Happiness Studies},
  volume = {21},
