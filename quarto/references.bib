@article{ngUsingPositivePsychological2022,
  title = {Using Positive Psychological Interventions to Improve Well-Being: {{Are}} They Effective across Cultures, for Clinical and Non-Clinical Samples?},
  shorttitle = {Using Positive Psychological Interventions to Improve Well-Being},
  author = {Ng, Weiting and Ong, Kuei Rong},
  date = {2022-03-01},
  journaltitle = {Journal of Contemporary Psychotherapy: On the Cutting Edge of Modern Developments in Psychotherapy},
  volume = {52},
  number = {1},
  pages = {45--53},
  publisher = {Springer US},
  location = {New York},
  issn = {0022-0116},
  doi = {10.1007/s10879-021-09513-8},
  abstract = {Substantial research has shown that positive psychological interventions (PPIs) have beneficial effects on well-being. This article evaluates whether PPIs would be effective for everyone, across different cultures. It starts by reviewing the effectiveness of PPIs among the general population, as well as their effectiveness in ameliorating depressive symptoms and increasing well-being among clinical samples. Though generally beneficial, PPIs however, may not be equally effective across cultures. We present evidence showing that certain types of PPIs may be less effective for Asians than those from Western cultures. Next, the article examines whether prosocial behavior may be a PPI that is universally beneficial and the possible explanations for its widespread benefits. Finally, we proceed to propose how the current repertoire of PPIs may be expanded to include new types that may be more suited for those in collectivistic cultures. Synthesizing across these areas, this article concludes with suggestions on how PPIs can be adapted for use across cultures, as well as future directions for the field of PPIs.},
  keywords = {Culture,Positive psychological interventions,Well-Being}
}

@article{nguyenChangesDigitalCommunication2020,
  title = {Changes in Digital Communication during the {{COVID-19}} Global Pandemic: {{Implications}} for Digital Inequality and Future Research},
  shorttitle = {Changes in Digital Communication during the {{COVID-19}} Global Pandemic},
  author = {Nguyen, Minh Hao and Gruber, Jonathan and Fuchs, Jaelle and Marler, Will and Hunsaker, Amanda and Hargittai, Eszter},
  date = {2020-07},
  journaltitle = {Social Media + Society},
  shortjournal = {Social Media + Society},
  volume = {6},
