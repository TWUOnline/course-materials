@article{abbasNegativeVsPositive2024,
  title = {Negative vs. {{Positive Psychology}}: A {{Review}} of {{Science}} of {{Well-Being}}},
  shorttitle = {Negative vs. {{Positive Psychology}}},
  author = {Abbas, Ansar and Ekowati, Dian and Suhariadi, Fendy and Hamid, Syed Ali Raza},
  date = {2024-12-01},
  journaltitle = {Integrative Psychological and Behavioral Science},
  shortjournal = {Integr. psych. behav.},
  volume = {58},
  number = {4},
  pages = {1091--1122},
  publisher = {Springer US},
  issn = {1936-3567},
  doi = {10.1007/s12124-022-09708-1},
  url = {https://link.springer.com/article/10.1007/s12124-022-09708-1},
  urldate = {2025-03-12},
  abstract = {Happiness, well-being, human freedom, and life events are interconnected. Nevertheless, the debate about human well-being struggles to find an exact definition. Literature debates on the importance of positive psychology or adverse effects of negative psychology in the well-being context discuss separately. However, both counter each other but have their significance and indisputable fact. Human psychology evolves around determinism and Free Will. One type of determinism is hard, while the other is soft. Individuals who are adamant about and embrace acceptance of chances are driven by their nature and psychology to choose negative behaviors under hard determinism. They give up their free will, whereas soft determinists use it to make choices and behave positively. However, the researchers looked at negative psychology as a useful aspect and positive psychology's dark side. We argued that there are reasons to suppose that chances can develop into options and vice versa. From a well-being perspective, negative and positive psychological strengths and weaknesses can be investigated. From the literature review, useful hypotheses for future research have been derived from our synthesis.},
  issue = {4},
  langid = {english},
  file = {/Users/chloe.chang/Zotero/storage/PFY458M9/Abbas et al. - 2024 - Negative vs. Positive Psychology a Review of Science of Well-Being.pdf}
}

