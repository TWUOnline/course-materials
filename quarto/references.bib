@article{snyderHopeTheoryRainbows2002,
  title = {Hope Theory: {{Rainbows}} in the Mind},
  shorttitle = {Hope Theory},
  author = {Snyder, C. R.},
  date = {2002},
  journaltitle = {Psychological Inquiry},
  volume = {13},
  number = {4},
  eprint = {1448867},
