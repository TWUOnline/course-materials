@video{teacherhubbytechsupportExploringBritishColumbias2023,
  entrysubtype = {video},
  title = {Exploring British Columbia's School Curriculum},
  editor = {{Teacher Hubby Tech Support}},
  editortype = {director},
  date = {2023-10-13},
  publisher = {YouTube},
  url = {https://www.youtube.com/watch?v=Gg_GbWDSLtY},
  urldate = {2025-07-31},
  abstract = {\#education  \#britishcolumbia  \#canadian  The British Colombia or BC curriculum in Canada which has been known for its high quality education back in the early 20th century.  The BC curriculum was designed with a simple principle in mind: to provide a balanced and comprehensive education to all students.  This was a time when education was becoming more accessible and the need for a standardised curriculum was becoming apparent. Over the years the curriculum evolved, incorporating new teaching methods and educational philosophies, always keeping up with the times.  By the mid 20th century the BC curriculum had established a reputation for providing a strong foundation in core subjects like Math, Science, English, and Social Studies.  Fast forward to the present day and the BC curriculum has only continued to adapt and grow. It now embraces a holistic approach to education focusing not only on academic knowledge but also on the development of critical thinking and problem-solving skills. It encourages students to be curious to ask questions and to develop a lifelong love for learning. The BC curriculum covers a wide range of subjects from the foundational ones like English, Math and Science to more specialized ones like Languages, Arts, Physical Education, and Technology. It is designed in a way that allows students to explore their interests and talents all the while ensuring they acquire essential skills and knowledge. The BC curriculum also places a strong emphasis on social responsibility and Global awareness.  Students learn about different cultures, histories and perspectives fostering empathy, open-mindedness and respect for diversity. The BC curriculum is recognized worldwide for its high standards and comprehensive approach to education. Universities around the globe acknowledge the BC diploma appreciating the well-rounded, knowledgeable and thoughtful individuals it produces.  The BC curriculum prepares students for University by equipping them with the necessary academic skills but it goes beyond that. It nurtures critical thinkers, problem solvers, and lifelong learners - qualities that are highly valued in higher education and the workplace. The BC curriculum has a rich history, a comprehensive and holistic content, and offers immense value to its students. It's not just about acquiring knowledge, it's about developing skills, nurturing curiosity, and fostering a love for learning. Whether it's preparing for University or life beyond that students who go through the BC curriculum are well equipped to face whatever comes their way.  So the next time you find yourself contemplating the importance of quality education remember the BC curriculum. It's a testament to the fact that a good education is not just about learning facts - it's about developing a well-rounded personality equipped with the skills and knowledge to navigate the world effectively and empathetically. Keep asking questions, keep learning, and remember - education is not just a journey: it's an adventure.}
}

@video{teacherhubbytechsupportScaffoldingBuildingBlocks2023,
  entrysubtype = {video},
  title = {Scaffolding: {{The}} Building Blocks of Education},
  shorttitle = {Scaffolding},
  editor = {{Teacher Hubby Tech Support}},
  editortype = {director},
  date = {2023-11-02},
  publisher = {YouTube},
  url = {https://www.youtube.com/watch?v=64aRIN817yQ},
  urldate = {2025-07-31},
  abstract = {\#education  Scaffolding in the Education System Scaffolding is a teaching strategy used in the elementary education system to support students as they learn new concepts and skills. It involves providing temporary support and guidance to help students reach higher levels of understanding and independence. Examples of Scaffolding in Education: 1. Breaking Down Tasks: Teachers may break down complex tasks into smaller, more manageable steps to help students grasp the overall concept. For example, when learning to write a paragraph, students may start with brainstorming ideas, then move on to organizing those ideas into sentences, and finally, combine the sentences to create a coherent paragraph. 2. Providing Prompts: Teachers may provide prompts or cues to help students think through a problem or answer a question. For example, when solving a math word problem, a teacher may ask guiding questions to help students identify the relevant information and choose the appropriate operation. 3. Modeling: Teachers may demonstrate a skill or concept to students before expecting them to do it on their own. For instance, when teaching a science experiment, a teacher may perform the experiment in front of the class, explaining each step and the underlying scientific principles. 4. Offering Graphic Organizers: Graphic organizers, such as concept maps or Venn diagrams, can help students visually organize their thoughts and make connections between different ideas. These tools provide a framework for students to structure their thinking and enhance their understanding of a topic. 5. Peer Collaboration: Encouraging students to work together in pairs or small groups can promote scaffolding. Students can support and learn from each other as they tackle a task or solve a problem. This collaborative approach fosters communication, critical thinking, and mutual support. Scaffolding plays a crucial role in helping students develop their skills, build confidence, and eventually become independent learners. By providing the necessary support and gradually removing it as students gain proficiency, scaffolding ensures a solid foundation for future academic success.}
}

