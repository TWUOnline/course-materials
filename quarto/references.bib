@article{chimaWhatItTakes2020,
  entrysubtype = {magazine},
  title = {What It Takes to Lead through an Era of Exponential Change},
  author = {Chima, Aneel and Gutman, Ron},
  date = {2020-10-29},
  journaltitle = {Harvard Business Review},
  issn = {0017-8012},
  url = {https://hbr.org/2020/10/what-it-takes-to-lead-through-an-era-of-exponential-change},
  urldate = {2024-08-09},
  abstract = {No, it isn’t just you — the pace of change has picked up. More than that, whereas we used to experience disruptions followed by periods of stability, change now is increasingly perpetual, pervasive, and exponential. To keep pace, leaders need to take a different approach than the “leader as hero” model — the solo, individualistic leader who inspires certainty in a deterministic way forward. The authors suggest a new approach, which they call Sapient Leadership, inspired by conversations held at Stanford in the spring of 2020 with Doug McMillon, President and CEO of Walmart; Toby Cosgrove, former CEO of the Cleveland Clinic, heart surgeon, and White House advisor; Halla Tómasdóttir, CEO of The B Team, and runner-up in Iceland’s 2016 presidential elections; and Bret Taylor, President and COO of Salesforce. These leaders articulate a humble, nimble, and value-driven vision for leadership. Leader humility, authenticity, and openness instills trust and psychological safety. In turn, trust and psychological safety empower individuals and teams to perform at their highest capabilities. Additionally, continuously learning teams are essential for keeping pace with and effectively navigating 3-D change. Finally, shared purpose and common values enhance focus, cohesion, and resilience in the midst of 3-D change.},
  keywords = {Adaptive leadership,Change management,Continuous learning},
  file = {/Users/chloe.chang/Zotero/storage/ADTTGEDB/what-it-takes-to-lead-through-an-era-of-exponential-change.html;/Users/chloe.chang/Zotero/storage/U3PBXCRI/what-it-takes-to-lead-through-an-era-of-exponential-change.html}
}

