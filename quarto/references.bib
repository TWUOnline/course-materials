@article{magyar-moeApplicationsPositivePsychology2015,
  title = {Applications of Positive Psychology in Counseling Psychology: {{Current}} Status and Future Directions},
  shorttitle = {Applications of Positive Psychology in Counseling Psychology},
  author = {Magyar-Moe, Jeana L. and Owens, Rhea L. and Scheel, Michael J.},
  date = {2015-05-01},
  journaltitle = {The Counseling Psychologist},
