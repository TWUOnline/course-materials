  booktitle = {Eighth {{International Conference}} on {{Technological Ecosystems}} for {{Enhancing Multiculturality}}},
  author = {Pocinho, Ricardo and Carrana, Pedro and Margarido, Cristovão and Santos, Rui and Milhano, Sandrina and Trindade, Bruno and Santos, Gisela},
