@article{jenningsNeuralHormonalControl2020,
  title = {Neural and Hormonal Control of Sexual Behavior},
  author = {Jennings, Kimberly J and family=Lecea, given=Luis, prefix=de, useprefix=true},
  date = {2020-08-26},
  journaltitle = {Endocrinology},
  shortjournal = {Endocrinology},
  volume = {161},
