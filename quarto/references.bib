@article{panaderoEffectsRubricsAcademic2023,
  title = {Effects of {{Rubrics}} on {{Academic Performance}}, {{Self-Regulated Learning}}, and Self-{{Efficacy}}: A {{Meta-analytic Review}}},
  author = {Panadero, Ernesto and Jonsson, Anders and Pinedo, Leire and Fern√°ndez-Castilla, Bel√©n},
  date = {2023-12-07},
  journaltitle = {Educational Psychology Review},
  shortjournal = {Educational Psychology Review},
  volume = {35},
