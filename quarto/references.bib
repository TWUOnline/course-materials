@inreference{KeystrokeLogging2024,
  title = {Keystroke Logging},
  booktitle = {Wikipedia},
  date = {2024-07-05},
  url = {https://en.wikipedia.org/w/index.php?title=Keystroke_logging&oldid=1232788853},
  abstract = {Keystroke logging, often referred to as keylogging or keyboard capturing, is the action of recording (logging) the keys struck on a keyboard, typically covertly, so that a person using the keyboard is unaware that their actions are being monitored. Data can then be retrieved by the person operating the logging program. A keystroke recorder or keylogger can be either software or hardware. While the programs themselves are legal, with many designed to allow employers to oversee the use of their computers, keyloggers are most often used for stealing passwords and other confidential information. Keystroke logging can also be utilized to monitor activities of children in schools or at home and by law enforcement officials to investigate malicious usage.    Keylogging can also be used to study keystroke dynamics or human-computer interaction. Numerous keylogging methods exist, ranging from hardware and software-based approaches to acoustic cryptanalysis.},
  langid = {english},
  annotation = {Page Version ID: 1232788853},
  file = {/Users/chloe.chang/Zotero/storage/CBRQBZXF/Keystroke_logging.html}
}

