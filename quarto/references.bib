@article{jensenAsynchronousOnlineInstruction2022,
  title = {Asynchronous {{Online Instruction Leads}} to {{Learning Gaps When Compared}} to a {{Flipped Classroom}}},
  author = {Jensen, Jamie and Smith, Clara M. and Bowers, Robert and Kaloi, Mahealani and Ogden, T. Heath and Parry, Kirkham A. and Payne, Joshua S. and Fife, Porter and Holt, Emily},
