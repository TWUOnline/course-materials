@article{edmondsonTodayLeadersNeed2020,
  entrysubtype = {magazine},
  title = {Today‚Äôs Leaders Need Vulnerability, Not Bravado},
  author = {Edmondson, Amy C. and Chamorro-Premuzic, Tomas},
  date = {2020-10-19T12:25:50Z},
  journaltitle = {Harvard Business Review},
  issn = {0017-8012},
